{
  "name": "My Chrome Extension",
  "version": "1.0",
  "manifest_version": 3,
  "description": "A Chrome extension with hash-based routing.",
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
    
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [],
  "host_permissions": ["<all_urls>"]
}
